{
  "rules": {
    "eofline": true,
    "import-spacing": true,
    "indent": [
      true,
      "spaces",
      2
    ],
    "max-line-length": [
      true,
      140
    ],
    "one-line": [
      true,
      "check-open-brace",
      "check-catch",
      "check-else",
      "check-whitespace"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "triple-equals": [
      true,
      "allow-null-check"
    ]
  }
}